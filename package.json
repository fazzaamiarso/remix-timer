{
  "name": "remix-timer",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "prisma generate && prisma migrate deploy && yarn build:css && remix build",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:remix\" ",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@prisma/client": "^3.12.0",
    "@remix-run/netlify": "^1.3.4",
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@tailwindcss/forms": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
